<article{{attributes.addClass('article-node')}}>

	<div{{content_attributes}}>

		<div class="field-content">
			<time>{{ node.created.value|date('d.m.Y') }}</time>
			{{ content.field_content_article }}
			<div class="tag-list">
				<hr/>
				<span>tags:</span> {{ content.field_tags }}
			</div>
		</div>

	</div>

</article>